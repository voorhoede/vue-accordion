{
  "name": "@voorhoede/vue-accordion",
  "version": "0.2.0",
  "description": "Behavioural and accessible accordion component",
  "author": "Voorhoede",
  "private": false,
  "license": "MIT",
  "main": "./dist/vueaccordion.common.js",
  "engines": {
    "node": ">= 10.16.0"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name vueaccordion ./src/app.vue",
    "lint": "vue-cli-service lint"
  },
  "homepage": "https://github.com/voorhoede/vue-accordion#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/voorhoede/vue-accordion"
  },
  "bugs": {
    "url": "https://github.com/voorhoede/vue-accordion/issues"
  },
  "keywords": [
    "accordion",
    "expandable",
    "vue",
    "vuejs",
    "accessible"
  ],
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "dependencies": {
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.2",
    "@vue/cli-plugin-eslint": "^3.9.2",
    "@vue/cli-service": "^3.9.2",
    "babel-eslint": "^10.0.2",
    "eslint": "^6.0.1",
    "eslint-plugin-vue": "^5.2.3",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
